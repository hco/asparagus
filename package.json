{
  "name": "miha-service-cron",
  "description": "miha bodytec logX portal",
  "private": true,
  "version": "1.2.12",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "deploy:stage": "sh -c \"cd deploy && ./deploy.sh stage\"",
    "deploy:test": "sh -c \"cd deploy && ./deploy.sh test\"",
    "deploy:prod": "sh -c \"cd deploy && ./deploy.sh prod\"",
    "postversion": "sh -c \"git push && git push --tags\"",
    "version:patch": "sh -c \"yarn version --new-version patch\"",
    "version:minor": "sh -c \"yarn version --new-version minor\"",
    "version:major": "sh -c \"yarn version --new-version major\"",
    "build": "docker build -t 570841197888.dkr.ecr.eu-central-1.amazonaws.com/miha-scheduler:$npm_package_version .",
    "login:aws": "$(aws ecr get-login --region eu-central-1 --no-include-email)",
    "push": "docker push 570841197888.dkr.ecr.eu-central-1.amazonaws.com/miha-scheduler:$npm_package_version",
    "release": "sh -c \"./build.py --release --platform=linux --arch=static_amd64\""
  }
}
